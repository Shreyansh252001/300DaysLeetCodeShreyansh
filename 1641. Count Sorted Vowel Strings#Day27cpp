 int countVowelStrings(int n) {
       
        int a=1,e=1,i=1,o=1,u=1;
        int i1=0;
        for(;i1<n;i1++)
        {
            o=o+u;
            i=i+o;
            e=e+i;
            a=a+e;
          //  cout<<a<<" "<<e<<" "<<i<<" "<<o<<" "<<u<<"\n";
        }
        
        return a;
    }
